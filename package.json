    "export:vercel": "ts-node --project tsconfig.scripts.json scripts/export-vercel-data.ts",
    "import:aws": "ts-node --project tsconfig.scripts.json scripts/import-aws-rds.ts"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"commonjs\"} prisma/seed.ts"
  },
